<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:id="@+id/imageViewLogoPartido"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/logopartido"
        android:layout_marginLeft="16dp"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:id="@+id/textViewNobrePartido"
        android:layout_alignParentTop="false"
        android:layout_toRightOf="@+id/imageViewLogoPartido"
        android:layout_toEndOf="@+id/imageViewLogoPartido"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="25dp" />





</RelativeLayout>